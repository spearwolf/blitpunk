{
  "name": "@spearwolf/blitpunk",
  "description": "todo",
  "version": "0.0.4",
  "scripts": {
    "standard": "standard",
    "mocha": "mocha --compilers js:babel-core/register",
    "esdoc": "esdoc",
    "lint:js": "npm run standard -- \"src/**/*.js\" \"test/**/*.js\" \"webgl-boilerplate/src/**/*.js\"",
    "test": "npm run lint:js && npm run mocha -- test/test_*.js",
    "boilerplate:build": "webpack --watch --config config/webpack.webgl-boilerplate.config.js",
    "boilerplate:watch": "npm run boilerplate:build -- --watch",
    "boilerplate:serve": "serve -p 8080 webgl-boilerplate",
    "start": "run-p boilerplate:serve boilerplate:watch"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.1.8",
    "esdoc": "^0.5.2",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.1",
    "serve": "^4.0.1",
    "standard": "^9.0.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "@spearwolf/eventize": "github:spearwolf/eventize",
    "@spearwolf/jab": "github:spearwolf/jab",
    "document-register-element": "^1.4.1",
    "uuid": "^3.0.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "config/*.js",
      "webgl-boilerplate/bundle.js"
    ]
  }
}
