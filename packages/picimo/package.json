{
  "name": "picimo",
  "version": "0.0.22",
  "description": "A typescript library for my personal visual gfx and game dev experiments based on three.js",
  "main": "dist/picimo.min.js",
  "module": "build/index.js",
  "types": "src/index.ts",
  "sideEffects": false,
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build:umd": "cross-env NODE_ENV=production rollup -c ./rollup.config.js",
    "build:esm": "cross-env BABEL_ENV=production babel src/ --extensions \".ts\" --out-dir ./build/ --source-maps",
    "watch:build:esm": "cross-env BABEL_ENV=production babel src/ --extensions \".ts\" --out-dir ./build/ --source-maps --watch",
    "build": "run-p build:*",
    "prepublishOnly": "npm-run-all --serial clean --parallel build:*",
    "clean": "rimraf build coverage dist",
    "test:eslint": "eslint .",
    "test:karma": "cross-env NODE_ENV=test karma start --single-run",
    "watch:test": "cross-env NODE_ENV=test karma start",
    "test:types": "tsc -p ./tests/types/tsconfig.json",
    "test": "npm-run-all --parallel test:eslint test:types --serial test:karma"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spearwolf/picimo.git"
  },
  "author": "Wolfger Schramm <wolfger@spearwolf.de>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/spearwolf/picimo/issues"
  },
  "homepage": "https://github.com/spearwolf/picimo#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "assert": "^2.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^6.0.0",
    "core-js": "^3.6.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-problems": "^3.1.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "lerna": "^3.20.2",
    "mocha": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.10.0",
    "rollup-plugin-terser": "^5.2.0",
    "three": "0.112.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "peerDependencies": {
    "three": "0.111.0"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.7.7",
    "@spearwolf/eventize": "spearwolf/eventize"
  }
}
