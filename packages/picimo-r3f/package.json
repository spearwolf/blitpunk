{
  "name": "picimo-r3f",
  "version": "0.0.1",
  "description": "Picimo react components based on the fabulous react-three-fiber",
  "module": "build/index.js",
  "types": "src/index.ts",
  "sideEffects": false,
  "scripts": {
    "build": "cross-env BABEL_ENV=production babel src/ --extensions \".ts\" --out-dir ./build/ --source-maps",
    "build:watch": "cross-env BABEL_ENV=production babel src/ --extensions \".ts\" --out-dir ./build/ --source-maps --watch",
    "prepublishOnly": "npm-run-all --serial clean --parallel build:*",
    "clean": "rimraf build coverage dist",
    "lint": "eslint .",
    "karma": "cross-env NODE_ENV=test karma start --single-run",
    "karma:watch": "cross-env NODE_ENV=test karma start",
    "test": "run-s lint karma"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spearwolf/picimo.git"
  },
  "author": "Wolfger Schramm <wolfger@spearwolf.de>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/spearwolf/picimo/issues"
  },
  "homepage": "https://github.com/spearwolf/picimo#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "assert": "^2.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "core-js": "^3.5.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.2",
    "eslint-config-problems": "^3.0.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "lerna": "^3.19.0",
    "mocha": "^6.2.2",
    "npm-run-all": "^4.1.5",
    "picimo": "*",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rimraf": "^3.0.0",
    "three": "0.111.0",
    "typescript": "^3.7.3",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "peerDependencies": {
    "picimo": "*",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "three": "0.111.0"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.7.6",
    "react-promise-suspense": "^0.3.2",
    "react-three-fiber": "^3.0.16"
  }
}
