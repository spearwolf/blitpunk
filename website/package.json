{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "^1.9.153",
    "gatsby-link": "^1.6.34",
    "gatsby-plugin-react-helmet": "^2.0.3",
    "gatsby-plugin-react-next": "^1.0.7",
    "gatsby-plugin-sass": "^1.0.15",
    "gatsby-plugin-styled-components": "^2.0.4",
    "gatsby-plugin-typography": "^1.7.10",
    "gatsby-remark-emoji": "^0.0.1",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^1.5.36",
    "gatsby-remark-prismjs": "^1.2.11",
    "gatsby-remark-responsive-iframe": "^1.4.16",
    "gatsby-source-filesystem": "^1.5.11",
    "gatsby-transformer-remark": "^1.7.28",
    "lodash": "^4.17.4",
    "open-color": "^1.6.2",
    "postcss-scss": "^1.0.3",
    "prism-themes": "^1.0.0",
    "react-helmet": "^5.2.0",
    "styled-components": "^2.4.0",
    "typography-breakpoint-constants": "^0.15.10",
    "typography-plugin-code": "^0.16.11",
    "typography-theme-parnassus": "^0.15.10",
    "typography-theme-wordpress-2016": "^0.15.10"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "babel-loader": "7.1.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "prettier": "^1.10.2",
    "webpack": "^3.10.0"
  },
  "eslintConfig": {
    "extends": [
      "airbnb"
    ],
    "globals": {
      "graphql": false
    },
    "rules": {
      "max-len": 128,
      "no-plusplus": 0,
      "consistent-return": 0,
      "import/no-extraneous-dependencies": 0,
      "react/no-danger": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [],
          "specialLink": [
            "hrefLeft",
            "hrefRight"
          ],
          "aspects": [
            "noHref",
            "invalidHref",
            "preferButton"
          ]
        }
      ]
    }
  }
}
